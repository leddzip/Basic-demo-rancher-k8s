;
; BIND data file for leddzip.io based domain
;
$TTL	604800
@	IN	SOA	leddzip. root.leddzip.io. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
	IN	A	192.168.100.250
;
@	IN	NS	ns.leddzip.io.
@	IN	A	192.168.100.250
@	IN	AAAA	::1
ns  IN  A   192.168.100.250
www IN  A   192.168.100.250
